<%- include('partials/head.ejs') %>

<!--Header-->

<div class="header">
  <img src="./images/sus.png" width="100">  
  <div class="header-right">   
    <a class="active" href="/index">Principal</a>
    <a href="/cadPct">Cadastrar Paciente</a>
    <a href="/buscaPct">Buscar Paciente</a> 
    <a href="/sobre">Sobre</a>
