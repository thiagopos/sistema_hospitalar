<h3 id='mensagemIndex'>Internações do Paciente</h3>
<!--Form-->
</div>
<div class="tabela">
  <% if(data !== null){ %>
    <table>
      <tr>
        <th>RH</th>
        <th>FAA</th>
        <th>Nome</th>
        <th>CPF</th>
        <th>Internação</th>
        <th>Clínica</th>
        <th>HD</th>        
      </tr>
    <% data.forEach(function(p) { %>    
        <tr>
          <td><%=p.rh%></td>
          <td><%=p.faa%></td>
          <td><%=p.nome%></td>
          <td><%=p.cpf%></td>
          <td><%=p.dataInter%></td>
          <td><%=p.clinica%></td>
          <td><%=p.hd%></td>                 
        </tr>    
    <% }) %>
  </table>
    <% } %>
</div>
 
  

